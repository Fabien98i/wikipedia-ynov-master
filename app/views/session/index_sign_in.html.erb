<main>
    <%= form_for :login do |f| %>
        <div>
            <%= f.label :email %>
            <%= f.text_field :email %>
        </div>
        <div>
            <%= f.label :password %>
            <%= f.password_field :password %>
        </div>
        <%= f.submit %>
    <% end %>
</main>
<br> <br>
<%= link_to 'New User', new_user_path %>